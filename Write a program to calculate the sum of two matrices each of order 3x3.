// Write a program to calculate the sum of two matrices each of order 3x3. 
#include<stdio.h>
#include<conio.h>

    void main()
{
    int first[3][3], second[3][3], sum[3][3];

    printf("Enter number of first matrix in row wise\n");
        for (int i = 0; i < 3; i++)
          for(int j=0; j<3; j++)
            scanf("%d",&first[i][j]);  
        
        for (int i = 0; i < 3; i++)   {
          for(int j=0; j<3; j++)
           printf("%d  ",first[i][j]);
            printf("\n");
        }

    printf("\nEnter number of second matrix in row wise\n");
        for (int i = 0; i < 3; i++)
            for(int j=0; j<3; j++)
                scanf("%d",&second[i][j]);  
            
            for (int i = 0; i < 3; i++)   {
            for(int j=0; j<3; j++)
            printf("%d  ",second[i][j]);
                printf("\n");
            }

    printf("sum of two matrix\n\n");
        for (int i = 0; i < 3; i++)
             for(int j=0; j<3; j++)
              sum[i][j]= first[i][j] + second[i][j];
                      
                
                for (int i = 0; i < 3; i++)   {
                for(int j=0; j<3; j++)
                printf("%d    ",sum[i][j]);
                    printf("\n\n");
                }


         
        
        
    getch();
}
