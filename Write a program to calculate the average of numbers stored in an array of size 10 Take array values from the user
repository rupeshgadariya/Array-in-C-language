// Write a program to calculate the average of numbers stored in an array of size 10.
//     Take array values from the user
#include<stdio.h>
#include<conio.h>

    void main()
{
        int ar[10],sum=0;
        float avg;
         
         printf("Enter 10 number ");
        for (int i = 0; i < 10; i++)
         scanf("%d",&ar[i]);

        for (int i = 0; i < 10; i++)
         sum=sum+ar[i];

         avg=sum/10;

         printf("Average of 10 number is %.2f ",avg);
        
        
        
    getch();
}
