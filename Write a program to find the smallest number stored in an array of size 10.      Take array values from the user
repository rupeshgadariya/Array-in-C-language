/*  Write a program to find the smallest number stored in an array of size 10. 
    Take array values from the user.    */
#include<stdio.h>
#include<conio.h>

    void main()
{
         int ar[10],temp;
        printf("Enter 10 number ");
        for (int i = 0; i < 10; i++)
         scanf("%d",&ar[i]);

         for (int i = 0; i < 10; i++)
            if( ar[i]<=ar[i+1] )            
{
                temp=ar[i+1];
                ar[i+1]=ar[i];
                ar[i]=temp;
            }
printf("%d is a smallest number ",ar[9]);
        
    getch();
}
