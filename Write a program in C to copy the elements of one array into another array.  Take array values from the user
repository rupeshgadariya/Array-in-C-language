/*  Write a program in C to copy the elements of one array into another array. 
    Take array values from the user */
#include<stdio.h>
#include<conio.h>

    void main()
{
        int ar[10],cp[10];
        printf("Enter 10 number ");
        for(int i=0; i<10; i++)
         scanf("%d",&ar[i]);

        for(int i=0; i<10; i++)
         cp[i] = ar[i];
        
        for(int i=0; i<10; i++)
        printf("%d  ",cp[i]);

        
    getch();
}
